    <div class="tab-pane fade show"
            id="my-tab-pane"
            role="tabpanel"
            aria-labelledby="my-tab">
        {# {% sw_include '@Storefront/storefront/page/product-detail/description.html.twig' %} #}
        {% sw_include '@MyNewPlugin/MyNewPlugin/page/product-detail/my-tab.html.twig' %}
    </div>
{% endblock %}